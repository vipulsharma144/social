  "dependencies": {
    "@kidkarolis/not-so-fast": "^1.0.1",
    "@koa/cors": "^3.1.0",
    "@prisma/client": "^4.3.1",
    "@types/koa": "^2.11.0",
    "@types/koa__cors": "^3.0.1",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-router": "^7.0.42",
    "@types/node": "^12.12.17",
    "@types/node-fetch": "^2.5.7",
    "@types/nodemailer": "^6.4.0",
    "@types/pg": "^7.11.2",
    "@types/uuid": "^8.0.0",
    "bcrypt": "^5.1.0",
    "bson": "^5.1.0",
    "crypto-js": "^4.1.1",
    "csv-string": "^4.0.1",
    "dotenv": "^16.0.3",
    "eventemitter2": "^6.3.1",
    "jsonwebtoken": "^9.0.0",
    "koa": "^2.11.0",
    "koa-bodyparser": "^4.2.1",
    "koa-easy-ws": "^1.3.0",
